{
    "name": "车辆属性识别",
    "id": "vehicle_attribute_recognition",
    "description": "车辆属性识别是计算机视觉系统中的重要组成部分，其主要任务是在图像或视频中定位并标记出车辆的特定属性，如车辆类型、颜色、车牌号等。该模块的性能直接影响到整个计算机视觉系统的准确性和效率。",
    "dataset_type": "ml_classification",
    "is_trainable": true,
    "models": {
        "PP-LCNet_x1_0_vehicle_attribute": {
            "description": "PP-LCNet_x1_0_vehicle_attribute 是一种基于PP-LCNet的轻量级车辆属性识别模型。",
            "model_size": "6.7 M"
        }
    },
    "infer_params": {
        "model_params": {
            "threshold": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": 0.99,
                "value": 0.5
            }
        },
        "predict_params": {
            "threshold": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": 1.0,
                "value": 0.5
            }
        },
        "input_types": [
            "img"
        ],
        "result_types": [
            "img",
            "json"
        ]
    }
}