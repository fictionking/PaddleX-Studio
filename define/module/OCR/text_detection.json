{
    "name": "文本检测",
    "id": "text_detection",
    "description": "文本检测模块是OCR（光学字符识别）系统中的关键组成部分，负责在图像中定位和标记出包含文本的区域。该模块的性能直接影响到整个OCR系统的准确性和效率。文本检测模块通常会输出文本区域的边界框（Bounding Boxes），这些边界框将作为输入传递给文本识别模块进行后续处理。",
    "is_trainable": true,
    "models": {
        "PP-OCRv5_server_det": {
            "description": "PP-OCRv5 的服务端文本检测模型，精度更高，适合在性能较好的服务器上部署",
            "model_size": "84.3 MB"
        },
        "PP-OCRv5_mobile_det": {
            "description": "PP-OCRv5 的移动端文本检测模型，效率更高，适合在端侧设备部署",
            "model_size": "4.7 MB"
        },
        "PP-OCRv4_server_det": {
            "description": "PP-OCRv4 的服务端文本检测模型，精度更高，适合在性能较好的服务器上部署",
            "model_size": "109 MB"
        },
        "PP-OCRv4_mobile_det": {
            "description": "PP-OCRv4 的移动端文本检测模型，效率更高，适合在端侧设备部署",
            "model_size": "4.7 MB"
        },
        "PP-OCRv3_mobile_det": {
            "description": "PP-OCRv3 的移动端文本检测模型，效率更高，适合在端侧设备部署",
            "model_size": "2.1 MB"
        },
        "PP-OCRv3_server_det": {
            "description": "PP-OCRv3 的服务端文本检测模型，精度更高，适合在性能较好的服务器上部署",
            "model_size": "102.1 MB"
        }
    },
    "infer_params": {
        "model_params": {
            "limit_side_len": {
                "config_able": true,
                "type": "int",
                "min": 0,
                "max": null,
                "value": null,
                "desc": "<span>检测的图像边长限制,如果设置为None, 将默认使用PaddleX官方模型配置中的该参数值</span>"
            },
            "limit_type": {
                "config_able": true,
                "type": "enum",
                "enum": [
                    null,
                    "min",
                    "max"
                ],
                "value": null,
                "desc": "<span>检测的边长限制类型，支持min和max. min表示保证图像最短边不小于det_limit_side_len, max: 表示保证图像最长边不大于limit_side_len</span>"
            },
            "thresh": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": null,
                "value": null,
                "desc": "<span>输出的概率图中，得分大于该阈值的像素点才会被认为是文字像素点</span>"
            },
            "box_thresh": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": null,
                "value": null,
                "desc": "<span>检测结果边框内，所有像素点的平均得分大于该阈值时，该结果会被认为是文字区域</span>"
            },
            "unclip_ratio": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": null,
                "value": null,
                "desc": "<span>Vatti clipping算法的扩张系数，使用该方法对文字区域进行扩张 </span>"
            }
        },
        "predict_params": {
            "limit_side_len": {
                "config_able": true,
                "type": "int",
                "min": 0,
                "max": null,
                "value": null,
                "desc": "<span>检测的图像边长限制,如果设置为None, 将默认使用PaddleX官方模型配置中的该参数值</span>"
            },
            "limit_type": {
                "config_able": true,
                "type": "enum",
                "enum": [
                    null,
                    "min",
                    "max"
                ],
                "value": null,
                "desc": "<span>检测的边长限制类型，支持min和max. min表示保证图像最短边不小于det_limit_side_len, max: 表示保证图像最长边不大于limit_side_len</span>"
            },
            "thresh": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": null,
                "value": null,
                "desc": "<span>输出的概率图中，得分大于该阈值的像素点才会被认为是文字像素点</span>"
            },
            "box_thresh": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": null,
                "value": null,
                "desc": "<span>检测结果边框内，所有像素点的平均得分大于该阈值时，该结果会被认为是文字区域</span>"
            },
            "unclip_ratio": {
                "config_able": true,
                "type": "float",
                "min": 0.0,
                "max": null,
                "value": null,
                "desc": "<span>Vatti clipping算法的扩张系数，使用该方法对文字区域进行扩张 </span>"
            }
        },
        "input_types": "img",
        "result_types": [
            "img",
            "json"
        ]
    }
}